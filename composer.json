{
    "name": "MatthewBeck/ContentWarning",
    "description": "Warn on incomplete or missing content",
    "type": "bolt-extension",
    "keywords": ["Content Warning"],
    "require": {
        "bolt/bolt": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Beck",
            "email": "mjb5@hotmail.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\MatthewBeck\\ContentWarning\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\MatthewBeck\\ContentWarning\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\MatthewBeck\\ContentWarning\\ContentWarningExtension"
    }
}
